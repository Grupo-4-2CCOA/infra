            "resultReuseEnabled": false,
            "resultReuseMaxAgeInMinutes": 60
          },
          "format": 1,
          "rawSQL": "WITH parsed AS (\r\n  SELECT\r\n    COALESCE(\r\n      TRY(from_iso8601_timestamp(data_hora_agendamento)),\r\n      TRY(date_parse(data_hora_agendamento, '%Y-%m-%d %H:%i:%s')),\r\n      TRY(date_parse(data_hora_agendamento, '%d/%m/%Y %H:%i:%s')),\r\n      TRY(date_parse(data_hora_agendamento, '%d/%m/%Y %H:%i'))\r\n    )                           AS ts,\r\n    UPPER(TRIM(status))         AS status_up,\r\n    TRIM(servico)               AS servico\r\n  FROM bb_flattened\r\n  WHERE data_hora_agendamento IS NOT NULL\r\n    AND servico IS NOT NULL\r\n),\r\nbase AS (\r\n  SELECT\r\n    date_trunc('month', ts)  AS mes_ts,\r\n    date_format(ts, '%Y-%m') AS mes_label,\r\n    servico\r\n  FROM parsed\r\n  WHERE ts IS NOT NULL\r\n    AND status_up = 'CANCELADO'\r\n),\r\nagg AS (\r\n  SELECT\r\n    mes_ts,\r\n    mes_label,\r\n    COUNT(*) AS total_mes,\r\n    SUM(CASE WHEN servico = 'Corte de Cabelo' THEN 1 ELSE 0 END) AS corte_de_cabelo,\r\n    SUM(CASE WHEN servico = 'Manicure'        THEN 1 ELSE 0 END) AS manicure,\r\n    SUM(CASE WHEN servico = 'Pedicure'        THEN 1 ELSE 0 END) AS pedicure,\r\n    SUM(CASE WHEN servico = 'Unhas de Gel'    THEN 1 ELSE 0 END) AS unhas_de_gel,\r\n    SUM(CASE WHEN servico = 'Limpeza de Pele' THEN 1 ELSE 0 END) AS limpeza_de_pele\r\n  FROM base\r\n  GROUP BY mes_ts, mes_label\r\n),\r\nranked AS (\r\n  SELECT\r\n    *,\r\n    ROW_NUMBER() OVER (ORDER BY mes_ts DESC) AS rn\r\n  FROM agg\r\n)\r\nSELECT\r\n  mes_label,\r\n  ROUND(100.0 * corte_de_cabelo / total_mes, 2) AS \"Corte de Cabelo (%)\",\r\n  ROUND(100.0 * manicure / total_mes, 2)        AS \"Manicure (%)\",\r\n  ROUND(100.0 * pedicure / total_mes, 2)        AS \"Pedicure (%)\",\r\n  ROUND(100.0 * unhas_de_gel / total_mes, 2)    AS \"Unhas de Gel (%)\",\r\n  ROUND(100.0 * limpeza_de_pele / total_mes, 2) AS \"Limpeza de Pele (%)\"\r\nFROM ranked\r\nWHERE rn <= 12\r\nORDER BY mes_ts ASC;\r\n",
          "refId": "A"
        }
      ],
      "title": "Taxa de Cancelamento por Serviço (por mês)",
      "type": "barchart"
    },
