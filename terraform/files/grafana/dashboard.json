              "format": 1,
              "rawSQL": "WITH parsed AS (\r\n  SELECT\r\n    COALESCE(\r\n      TRY(from_iso8601_timestamp(f.data_hora_agendamento)),\r\n      TRY(date_parse(f.data_hora_agendamento, '%d/%m/%Y %H:%i')),\r\n      TRY(date_parse(f.data_hora_agendamento, '%Y-%m-%d %H:%i:%s')),\r\n      TRY(date_parse(f.data_hora_agendamento, '%Y-%m-%d'))\r\n    ) AS ts,\r\n    UPPER(f.status) AS status_up,\r\n    f.feriado\r\n  FROM bb_flattened f\r\n  WHERE f.status IS NOT NULL\r\n),\r\nbase AS (\r\n  SELECT\r\n    DATE(date_trunc('day', ts)) AS data_agendamento,\r\n    status_up,\r\n    feriado\r\n  FROM parsed\r\n  WHERE ts IS NOT NULL\r\n),\r\nferiados AS (\r\n  SELECT DISTINCT data_agendamento AS data_feriado\r\n  FROM base\r\n  WHERE feriado = 'Sim'\r\n),\r\nclassificado AS (\r\n  SELECT\r\n    b.data_agendamento,\r\n    b.status_up,\r\n    CASE\r\n      WHEN b.feriado = 'Sim' THEN 'Feriado'\r\n      WHEN DATE_ADD('day', 1, b.data_agendamento) IN (SELECT data_feriado FROM feriados)\r\n           THEN 'Véspera de feriado'\r\n      ELSE 'Dia útil'\r\n    END AS tipo_dia\r\n  FROM base b\r\n),\r\nagg AS (\r\n  SELECT\r\n    tipo_dia,\r\n    COUNT(*) AS total_agend,\r\n    SUM(CASE WHEN status_up IN ('CANCELADO','CANCELADA') THEN 1 ELSE 0 END) AS cancelados\r\n  FROM classificado\r\n  GROUP BY tipo_dia\r\n)\r\nSELECT\r\n  tipo_dia,\r\n  ROUND((CAST(cancelados AS DOUBLE) / NULLIF(total_agend, 0)) * 100, 2)\r\n    AS \"Percentual Agendamentos de Cancelamentos\"\r\nFROM agg\r\nORDER BY CASE tipo_dia\r\n  WHEN 'Feriado' THEN 1\r\n  WHEN 'Véspera de feriado' THEN 2\r\n  WHEN 'Dia útil' THEN 3\r\n  ELSE 4\r\nEND;\r\n",
              "refId": "A"
            }
