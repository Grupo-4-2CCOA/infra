              "format": 1,
              "rawSQL": "WITH parsed AS (\r\n  SELECT\r\n    COALESCE(\r\n      TRY(from_iso8601_timestamp(f.data_hora_agendamento)),\r\n      TRY(date_parse(f.data_hora_agendamento, '%d/%m/%Y %H:%i'))\r\n    ) AS ts,\r\n    TRIM(f.status) AS status_up\r\n  FROM bb_flattened f\r\n  WHERE f.status IS NOT NULL\r\n),\r\nbase AS (\r\n  SELECT\r\n    day_of_week(ts) AS dia_semana_num,\r\n    status_up\r\n  FROM parsed\r\n  WHERE ts IS NOT NULL AND day_of_week(ts) <> 7\r\n),\r\nagg AS (\r\n  SELECT\r\n    dia_semana_num,\r\n    COUNT(*) AS total_agend,\r\n    SUM(CASE WHEN status_up = 'Cancelado' THEN 1 ELSE 0 END) AS cancelados\r\n  FROM base\r\n  GROUP BY dia_semana_num\r\n)\r\nSELECT\r\n  CASE dia_semana_num\r\n    WHEN 1 THEN 'Segunda-feira'\r\n    WHEN 2 THEN 'Terça-feira'\r\n    WHEN 3 THEN 'Quarta-feira'\r\n    WHEN 4 THEN 'Quinta-feira'\r\n    WHEN 5 THEN 'Sexta-feira'\r\n    WHEN 6 THEN 'Sábado'\r\n  END AS dia_semana,\r\n  ROUND((CAST(cancelados AS DOUBLE) / NULLIF(total_agend, 0)) * 100, 2) AS \"Percentual de Agendamento Cancelados\"\r\nFROM agg\r\nORDER BY dia_semana_num;\r\n",
