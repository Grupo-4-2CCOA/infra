              "rawSQL": "WITH params AS (\r\n  SELECT\r\n    1.0  AS lim_chuva_mm,\r\n    34.0 AS lim_calor_c,\r\n    10.0 AS lim_frio_c\r\n),\r\nparsed AS (\r\n  SELECT\r\n    COALESCE(\r\n      TRY(from_iso8601_timestamp(f.data_hora_agendamento)),\r\n      TRY(date_parse(f.data_hora_agendamento, '%d/%m/%Y %H:%i')),\r\n      TRY(date_parse(f.data_hora_agendamento, '%Y-%m-%d %H:%i:%s')),\r\n      TRY(date_parse(f.data_hora_agendamento, '%Y-%m-%d'))\r\n    ) AS ts,\r\n    CAST(f.Precipitacao AS DOUBLE) AS precip_mm,\r\n    CAST(f.Temperatura  AS DOUBLE) AS temp_c,\r\n    NULLIF(TRIM(f.servico), '')     AS servico\r\n  FROM bb_flattened f\r\n),\r\nclassif AS (\r\n  SELECT\r\n    servico,\r\n    CASE\r\n      WHEN precip_mm >= p.lim_chuva_mm THEN 'Chuvoso'\r\n      WHEN temp_c   >= p.lim_calor_c   THEN 'Calor'\r\n      WHEN temp_c   <= p.lim_frio_c    THEN 'Frio'\r\n      ELSE 'Ensolarado'\r\n    END AS tipo_clima\r\n  FROM parsed\r\n  CROSS JOIN params p\r\n  WHERE ts IS NOT NULL\r\n    AND servico IS NOT NULL\r\n),\r\nagg AS (\r\n  SELECT\r\n    servico,\r\n    SUM(CASE WHEN tipo_clima = 'Chuvoso'    THEN 1 ELSE 0 END) AS chuvoso,\r\n    SUM(CASE WHEN tipo_clima = 'Calor'      THEN 1 ELSE 0 END) AS calor,\r\n    SUM(CASE WHEN tipo_clima = 'Frio'       THEN 1 ELSE 0 END) AS frio,\r\n    SUM(CASE WHEN tipo_clima = 'Ensolarado' THEN 1 ELSE 0 END) AS ensolarado\r\n  FROM classif\r\n  GROUP BY servico\r\n)\r\nSELECT\r\n  servico,\r\n  ensolarado AS \"Ensolarado\",\r\n  chuvoso AS \"Chuvoso\",\r\n  calor AS \"Calor (>= 34°C)\",\r\n  frio AS \"Frio (<= 10°C)\"\r\nFROM agg\r\nORDER BY (chuvoso + calor + frio + ensolarado) DESC;\r\n",
