              "format": 1,
              "rawSQL": "WITH params AS (\r\n  SELECT\r\n    1.0  AS lim_chuva_mm,   -- precipitação ≥ 1.0 mm => Chuvoso\r\n    32.0 AS lim_calor_c,    -- temperatura ≥ 32 °C   => Calor extremo\r\n    10.0 AS lim_frio_c      -- temperatura ≤ 10 °C   => Frio extremo\r\n),\r\nparsed AS (\r\n  SELECT\r\n    COALESCE(\r\n      TRY(from_iso8601_timestamp(f.data_hora_agendamento)),         \r\n      TRY(date_parse(f.data_hora_agendamento, '%d/%m/%Y %H:%i')),\r\n      TRY(date_parse(f.data_hora_agendamento, '%Y-%m-%d %H:%i:%s')),\r\n      TRY(date_parse(f.data_hora_agendamento, '%Y-%m-%d'))\r\n    ) AS ts,\r\n    f.Precipitacao           AS precip_mm,\r\n    f.Temperatura            AS temp_c,\r\n    UPPER(f.status)          AS status_up\r\n  FROM bb_flattened f\r\n),\r\nbase AS (\r\n  SELECT\r\n    DATE(date_trunc('day', ts)) AS dt,\r\n    precip_mm,\r\n    temp_c,\r\n    status_up,\r\n    CASE\r\n      WHEN precip_mm >= p.lim_chuva_mm THEN 'Chuvoso'\r\n      WHEN temp_c   >= p.lim_calor_c   THEN 'Calor extremo'\r\n      WHEN temp_c   <= p.lim_frio_c    THEN 'Frio extremo'\r\n      ELSE 'Ensolarado'\r\n    END AS tipo_clima\r\n  FROM parsed\r\n  CROSS JOIN params p\r\n  WHERE ts IS NOT NULL\r\n),\r\nagg AS (\r\n  SELECT\r\n    tipo_clima,\r\n    COUNT(*) AS total_agend,\r\n    SUM(CASE WHEN status_up IN ('CANCELADO','CANCELADA') THEN 1 ELSE 0 END) AS cancelados\r\n  FROM base\r\n  GROUP BY tipo_clima\r\n)\r\nSELECT\r\n  tipo_clima,\r\n  ROUND(\r\n    (CAST(cancelados AS DOUBLE) / NULLIF(total_agend, 0)) * 100,\r\n    2\r\n  ) AS \"Percentual de Agendamentos Cancelados\"\r\nFROM agg\r\nORDER BY CASE tipo_clima\r\n  WHEN 'Chuvoso'       THEN 1\r\n  WHEN 'Calor extremo' THEN 2\r\n  WHEN 'Frio extremo'  THEN 3\r\n  ELSE 4\r\nEND;\r\n",
              "refId": "A",
              "table": "bb_flattened"
            }
          ],
          "title": "Taxa de Agendamentos Cancelados em Relação ao Clima",
          "type": "barchart"
        },
